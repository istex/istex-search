{
  "name": "istex-dl",
  "version": "5.2.1",
  "description": "Web application dedicated to ISTEX sub-corpus extraction.",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "npm run lint && vitest run",
    "lint": "eslint --ignore-path .gitignore . --ext .js,.jsx",
    "docker:build": "docker build -t istex/istex-dl:$npm_package_version --build-arg http_proxy --build-arg https_proxy .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose stop"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.11",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.3",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.29.3",
    "eventemitter3": "^5.0.0",
    "flowbite": "^1.6.3",
    "flowbite-react": "^0.1.3",
    "inist-ark": "^2.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.1",
    "react-select": "^5.7.0",
    "react-textarea-autosize": "^8.4.0",
    "react-toastify": "^9.1.1"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "^0.4.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-config-standard-react": "^12.0.0",
    "jsdom": "^21.1.0",
    "postcss": "^8.4.21",
    "prop-types": "^15.8.1",
    "sass": "^1.58.3",
    "tailwindcss": "^3.2.7",
    "vite": "^4.1.4",
    "vite-plugin-svgr": "^2.4.0",
    "vitest": "^0.28.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  }
}
